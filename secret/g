javascript:(function(){            var expression = prompt('Enter an arithmetic expression to calculate (e.g., 6+12/2):');        if (expression !== null && expression !== '') {                try {                        var result = eval(expression);            alert('Result: ' + result);                } catch (error) {                        console.error('Execution Error:', error);                }        } else {                console.log('No expression provided or cancelled.');        }})();
